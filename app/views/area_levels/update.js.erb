<% if @area_level.errors.empty? %>
  // add the row to the table, show alert and hide modal
  $('table tbody tr#area_level_<%= @area_level.id %>').replaceWith(' <%= j render "row", area_level: @area_level %>');
  //$('.alert-success').html("El area_level ha sido modificado con Ã©xito").show();
  $('#area_level-modal').modal('hide');
<% else %>
  // show first error
  alert('<%= @area_level.errors.full_messages[0] %>');
<% end %>
